<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:th="http://www.thymeleaf.org">
    <th:block th:with="absoluteUrl=${#httpServletRequest.scheme}+'://'+${#httpServletRequest.serverName}+${#httpServletRequest.contextPath}">
        <th:block th:with="currentSeason=${T(com.nephest.battlenet.sc2.web.service.blizzard.BlizzardSC2API).LAST_SEASON}">
            <th:block th:each="season : ${T(com.nephest.battlenet.sc2.web.service.blizzard.BlizzardSC2API).MMR_SEASONS.keySet()
                .stream().sorted().toArray()}">
                <th:block th:each="queueType : ${T(com.nephest.battlenet.sc2.model.QueueType).getTypes(T(com.nephest.battlenet.sc2.model.Version).LOTV)}">
                    <th:block th:each="teamType : ${T(com.nephest.battlenet.sc2.model.TeamType).values()}">
                        <url th:if="${T(com.nephest.battlenet.sc2.web.service.blizzard.BlizzardSC2API)
                            .isValidCombination(null, queueType, teamType)}">
                            <loc th:text="@{__${absoluteUrl}__/
                            (season=${season},queue=${queueType.toString()},team-type=${teamType.toString()}
                            ,us='true',eu='true',kr='true',cn='true'
                            ,bro='true',sil='true',gol='true',pla='true',dia='true',mas='true',gra='true'
                            ,page='0',type='ladder',ratingAnchor='99999',idAnchor='0',forward='true',count='1'
                            ,t='generator',t='ladder-top')}"></loc>
                            <changefreq th:text="${season == currentSeason ? 'daily' : 'never'}"></changefreq>
                            <priority th:text="${season == currentSeason ? '1.0' : 0.1}"></priority>
                        </url>
                    </th:block>
                </th:block>
            </th:block>
        </th:block>
    </th:block>
</urlset>
